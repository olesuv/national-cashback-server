{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "crons": [
    {
      "schedule": "0 3 * * 6",
      "path": "/"
    }
  ],
  "functions": {
    "dist/main.js": {
      "maxDuration": 90
    }
  }
}
