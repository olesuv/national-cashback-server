{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "HEAD", "OPTIONS"]
    }
  ],
  "crons": [
    {
      "name": "csv-to-sql-migration",
      "schedule": " 0 3 * * 6",
      "path": "/"
    }
  ]
}
